          "editorMode": "code",
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "WITH latest_data AS (\n  SELECT \n    ship_id,\n    cargo_id,\n    value,\n    ROW_NUMBER() OVER (PARTITION BY ship_id, cargo_id ORDER BY time DESC) as rn\n  FROM cargo_data \n  WHERE $__timeFilter(time)\n    AND ship_id IN ($airport)\n    AND cargo_id IN (\n      'Temperature_Celsius', \n      'Humidity_Percent', \n      'Pressure_hPa', \n      'Wind_Speed_Knots', \n      'UV_Index'\n    )\n)\nSELECT \n  ship_id as \"Airport\",\n  MAX(CASE WHEN cargo_id = 'Temperature_Celsius' THEN value END) as \"Temperature\",\n  MAX(CASE WHEN cargo_id = 'Humidity_Percent' THEN value END) as \"Humidity\", \n  MAX(CASE WHEN cargo_id = 'Pressure_hPa' THEN value END) as \"Pressure\",\n  MAX(CASE WHEN cargo_id = 'Wind_Speed_Knots' THEN value * 1.852 END) as \"Wind Speed (km/h)\",\n  MAX(CASE WHEN cargo_id = 'UV_Index' THEN value END) as \"UV Index\"\nFROM latest_data \nWHERE rn = 1\nGROUP BY ship_id\nORDER BY ship_id;\n",
          "refId": "A",
          "select": [
            [
              {
                "params": [
                  "value"
                ],
                "type": "column"
              }
            ]
          ],
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          },
          "timeColumn": "time",
          "where": [
            {
              "name": "$__timeFilter",
              "params": [],
              "type": "macro"
            }
          ]
        }
      ],
      "title": "Current Weather Conditions",
      "type": "table"
